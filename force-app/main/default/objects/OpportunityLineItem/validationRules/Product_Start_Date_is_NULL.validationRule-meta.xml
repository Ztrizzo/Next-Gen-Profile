<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Product_Start_Date_is_NULL</fullName>
    <active>true</active>
    <description>Product requires a Start Date</description>
    <errorConditionFormula>AND(ISBLANK(ServiceDate),NOT($Profile.Name = &quot;System Administrator&quot;))</errorConditionFormula>
    <errorDisplayField>ServiceDate</errorDisplayField>
    <errorMessage>Start Date is required.</errorMessage>
</ValidationRule>
